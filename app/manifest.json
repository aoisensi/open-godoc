{
  "name": "Open godoc.org",
  "short_name": "Open godoc.org",
  "version": "0.1.0.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/gophercolor-16.png",
    "48": "images/gophercolor-48.png",
    "128": "images/gophercolor-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "images/gophercolor-19.png"
    },
    "default_title": "__MSG_pageActionTitle__"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "http://godoc.org/*"
  ],
  "author": "aoisensi",
  "homepage_url": "http://github.com/aoisensi/open-godoc"
}